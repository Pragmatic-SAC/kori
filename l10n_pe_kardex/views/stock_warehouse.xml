<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="pragmatic_stock_warehouse" model="ir.ui.view">
        <field name="name">stock.warehouse.inherit</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">
            <field name="code" position="after">
                <field name="show_field" invisible="1"/>
                <field name="establishment" options="{'no_create': True, 'no_create_edit':True,'no_open': True}" attrs="{'invisible': [('show_field', '=', False)]}"/>
            </field>
        </field>
    </record>
</odoo>